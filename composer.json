		"navruzm/lmongo": "*",
		"navruzm/laravel-mongo-auth": "*",
		"flatline/mongol": "0.1.*",
		"monga/monga": "dev-master",
		"julien-c/mongovel": "*",
		"anahkiasen/former": "dev-agnostic",
